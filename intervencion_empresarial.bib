@thesis{castrodiazPropuestaDisenoMetodologia2023,
  title = {Propuesta de diseño de una metodología para la gestión de proyectos en la empresa Ingeniería, Montajes y Mantenimiento S.A.S.},
  author = {Castro Díaz, Víctor Alfonso and Giraldo Sabogal, Juan David and Gonzáles Suarez, Zaira Zamari},
  date = {2023-12-12},
  url = {http://hdl.handle.net/10882/13283},
  urldate = {2025-05-22},
  abstract = {El trabajo de grado se centra en diseñar una metodología específica para mejorar la gestión de proyectos en la empresa Ingeniería, Montajes y Mantenimiento S.A.S. Tras una exhaustiva revisión de literatura sobre estándares y metodologías de gestión de proyectos, se desarrolló una metodología personalizada basada en los principios de PMBOK, PM2 y PRINCE 2, adaptada a las necesidades de la empresa. El proceso incluyó un análisis situacional, revisión de proyectos ejecutados y entrevistas con los propietarios. La metodología propuesta tiene un ciclo de vida de proyecto de cinco fases, roles y responsabilidades claros, y un enfoque de gestión de calidad. Se propuso un plan de implementación que abarcó la capacitación del personal, la entrega de artefactos y talleres prácticos. El resultado es una metodología personalizada con el objetivo de reducir sobrecostos, mejorar la calidad de proyectos y aumentar la reputación en el mercado. Este estudio puede ser referencial para otras organizaciones, especialmente PYMEs, que buscan mejorar sus prácticas de gestión de proyectos.},
  langid = {spanish},
  keywords = {empresa de ingenieria,metodologia,Propuesta},
  file = {/Users/maser/Zotero/storage/7TIZ73ZQ/Castro Díaz et al. - 2023 - Propuesta de diseño de una metodología para la gestión de proyectos en la empresa Ingeniería, Montaj.pdf}
}

@article{contreras-verbelAplicacionModeloMadurez2022,
  title = {Aplicación del Modelo de Madurez organizacional OPM3 en Empresas colombianas},
  author = {Contreras-Verbel, Adalberto M.},
  date = {2022-12-23},
  journaltitle = {Revista científica anfibios},
  volume = {5},
  number = {2},
  pages = {45--56},
  issn = {2711-0532},
  doi = {10.37979/afb.2022v5n2.112},
  url = {https://www.revistaanfibios.org/ojs/index.php/afb/article/view/112},
  urldate = {2025-05-22},
  abstract = {Objective: The objective of this work was to know the current status of the application of the OPM3 organizational maturity model in Colombian companies. Method: A systematic review of information in reference databases was carried out to search for scientific, review and reflection articles, as well as books and graduate works or theses dealing with the subject in the period from 2012 to 2022. Results: 14 scientific papers were obtained as results, most of them from studies conducted in Bogota and Cali, which showed both in public and private companies found a low organizational maturity presenting average values of 52\%. Conclusions: The use of the OPM3 model was evidenced in several types of companies in Colombia, companies of different sizes, number of employees, geographical location and context, which demonstrates the efficiency of the model for any company. On the other hand, in spite of the low level of organizational maturity, the results obtained from the model allowed these companies to carry out improvement plans for a continuous increase and to present better productivity indexes and better positioning in relation to market competencies.},
  issue = {2},
  langid = {spanish},
  keywords = {Cultura organizacional,Estudios organizacionales,Gerencia de proyectos,Modelos de madurez},
  file = {/Users/maser/Zotero/storage/HGHMTHQK/Contreras-Verbel - 2022 - Aplicación del Modelo de Madurez organizacional OPM3 en Empresas colombianas.pdf}
}

