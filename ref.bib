	publisher = {Quito, Ecuador: Editorial UISRAEL},
	author = {Ochoa Guevara, Nancy Edith and Caballero Palomino, Sergio Andrés and Ochoa Guevara, Sandra Patricia and Acuña Reyes, Deisy Mireya},
	month = jul,
	year = {2022},
